{
  "name": "OECD.Direct",
  "description": "Integration of MessageFocus with OECD Publications",
  "version": "3.3.0",
  "author": "Jakob Fix <jakob.fix@gmail.com>",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://app.adestra.com/*/html_editor_new",
        "*://app.adestra.com/*/email_editor"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "icons": {
    "48": "assets/oecd-logo-48.png",
    "128": "assets/oecd-logo-128.png"
  },
  "manifest_version": 2
}
